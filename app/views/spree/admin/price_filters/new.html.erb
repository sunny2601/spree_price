<%= render :partial => 'spree/admin/shared/configuration_menu' %>

<div data-hook="admin_price_filter_new_form_header">
  <h1><%= Spree.t("new_price_filter") %></h1>

  <%= render "spree/shared/error_messages", :target => @price_filter %>
</div>

<div data-hook="admin_price_filter_new_form" >
  <%= form_for(:price_filter, :url => collection_url) do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>

    <div data-hook="admin_price_filter_new_form_buttons">
      <%= render :partial => "spree/admin/shared/new_resource_links" %>
    </div>
  <% end %>
</div>
